// Brute force Approach

#include <bits/stdc++.h> 
vector<int> maxMinWindow(vector<int> a, int n) {
    vector<int> ans(n, INT_MIN);
    for(int i=0; i<n; i++) {
        for(int j=i; j<n; j++) {
            int len = j-i+1;
            int temp = INT_MAX;
            for(int k=i; k<=j; k++) {
                temp = min(temp, a[k]);
            }
            ans[len-1] = max(ans[len-1], temp);
        }
    }
    return ans;
}

